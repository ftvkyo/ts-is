{
  "name": "ts-is",
  "version": "0.0.1",
  "description": "Typescript runtime typecheck generator",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ftvkyo/ts-is"
  },
  "author": "Andrei Stepanenko",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json && tslint --project tsconfig.test.json",
    "fix": "tslint --project tsconfig.json --fix && tslint --project tsconfig.test.json --fix",
    "test": "yarn run lint && yarn run build && ttsc --project tsconfig.test.json && jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.2"
  }
}
